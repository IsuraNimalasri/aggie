{
  "name": "aggie",
  "version": "2.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/TID-Lab/aggie.git"
  },
  "scripts": {
    "postinstall": "gulp build",
    "prestart": "node install.js",
    "start": "node app.js",
    "premocha": "node install.js",
    "mocha": "mocha test/backend",
    "karma": "karma start --single-run",
